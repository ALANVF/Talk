#!perl6

loop from 1 to 100 as i
	i is now i.Str
	
	if not(i.chop(2).starts-with("1")) and i.chop.ends-with(one <1 2 3>)
		if i.ends-with "1"
			print i + "st "
		also if i.ends-with "2"
			print i + "nd "
		also if i.ends-with "3"
			print i + "rd "
		end
	otherwise
		print i + "th "
	end
	
	if i.Int % 10 eq 0
		say newline
	end
end

